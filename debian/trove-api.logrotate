/var/log/trove/api.log {
    daily
    missingok
    compress
    delaycompress
    notifempty
    postrotate
        if dpkg-vendor --derives-from ubuntu ; then
            service trove-api restart >/dev/null 2>&1 || true
        else
            invoke-rc.d trove-api restart >/dev/null 2>&1 || true
        fi
    endscript
}
