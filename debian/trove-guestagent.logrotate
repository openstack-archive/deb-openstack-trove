/var/log/trove/guestagent.log {
    daily
    missingok
    compress
    delaycompress
    notifempty
    postrotate
        if dpkg-vendor --derives-from ubuntu ; then
            service trove-guestagent restart >/dev/null 2>&1 || true
        else
            invoke-rc.d trove-guestagent restart >/dev/null 2>&1 || true
        fi
    endscript
}
